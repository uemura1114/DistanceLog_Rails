<%= form_with model: @distance do |f| %>
  <%= f.label :st_lat, 'Start Lat' %>
  <%= f.number_field :st_lat, step: 0.000001, class: 'form-control' %>
  <%= f.label :st_lng, 'Start Lng' %>
  <%= f.number_field :st_lng, step: 0.000001, class: 'form-control' %>
  <%= f.label :ed_lat, 'End Lat' %>
  <%= f.number_field :ed_lat, step: 0.000001, class: 'form-control' %>
  <%= f.label :ed_lng, 'End Lng' %>
  <%= f.number_field :ed_lng, step: 0.000001, class: 'form-control' %>
  <%= f.submit '登録' %>
<% end %>